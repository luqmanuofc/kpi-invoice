# Frontend Environment Variables
# Password for login page
VITE_APP_PASSWORD=your-secure-password-here

# Auth token that gets stored in localStorage after successful login
# This should match the AUTH_TOKEN on the backend
VITE_AUTH_TOKEN=your-secure-auth-token-here

# Backend Environment Variables (Netlify)
# Auth token to validate API requests
# This should match the VITE_AUTH_TOKEN on the frontend
AUTH_TOKEN=your-secure-auth-token-here

# Database connection string
DATABASE_URL=your-database-connection-string
